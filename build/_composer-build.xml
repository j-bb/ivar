<?xml version="1.0" encoding="UTF-8"?>

<project name="composer build">

	<target name="_qx-composer-clean">
		<stopwatch name="qx-composer-clean" />
		<antcall target="_py-exec">
			<param name="qx-action" value="clean" />
			<param name="py-exec-directory" value="../composer/qx" />
		</antcall>
		<antcall target="_py-exec">
			<param name="qx-action" value="distclean" />
			<param name="py-exec-directory" value="../composer/qx" />
		</antcall>
		<stopwatch name="qx-composer-clean" action="total" />
	</target>

	<target name="_qx-composer-build">
		<stopwatch name="qx-composer-build" />
		<antcall target="_py-exec">
			<param name="qx-action" value="build" />
			<param name="py-exec-directory" value="../composer/qx" />
		</antcall>
		<stopwatch name="qx-composer-build" action="total" />
	</target>

	<target name="_qx-composer-source" >
		<stopwatch name="qx-composer-source" />
		<antcall target="_py-exec">
			<param name="qx-action" value="source" />
			<param name="py-exec-directory" value="../composer/qx" />
		</antcall>
		<stopwatch name="qx-composer-source" action="total" />
	</target>

	<target name="_qx-composer-source-all" >
		<stopwatch name="qx-composer-source-all" />
		<antcall target="_py-exec">
			<param name="qx-action" value="source-all" />
			<param name="py-exec-directory" value="../composer/qx" />
		</antcall>
		<stopwatch name="qx-composer-source-all" action="total" />
	</target>
</project>